{
  "kind": "collectionType",
  "collectionName": "schools",
  "info": {
    "name": "School",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "county": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "classrooms": {
      "via": "school",
      "collection": "classroom"
    },
    "mentors": {
      "via": "school",
      "collection": "mentor"
    },
    "Administrator": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
